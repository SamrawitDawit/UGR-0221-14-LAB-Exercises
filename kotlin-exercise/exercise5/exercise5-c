fun main() {
    val number = 12321
    var temp = number
    var reversedNumber = 0
    var remainder: Int

    while (temp != 0) {
        remainder = temp % 10
        reversedNumber = reversedNumber * 10 + remainder
        temp /= 10
    }

    if (number == reversedNumber) {
        println("$number is a palindrome")
    } else {
        println("$number is not a palindrome")
    }
}
