fun main() {
    val start = 10
    val end = 50

    var num = start

    while (num <= end) {
        var isPrime = true
        var i = 2
        while (i <= num / 2) {
            if (num % i == 0) {
                isPrime = false
                break
            }
            i++
        }
        if (isPrime && num > 1) {
            println(num)
        }
        num++
    }
}
