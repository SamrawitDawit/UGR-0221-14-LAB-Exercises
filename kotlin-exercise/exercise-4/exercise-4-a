fun sortArray(arr: IntArray): IntArray {
    for (i in arr.indices) {
        for (j in i + 1 until arr.size) {
            if (arr[i] > arr[j]) {
                val temp = arr[i]
                arr[i] = arr[j]
                arr[j] = temp
            }
        }
    }
    return arr
}

fun main() {
    val array = intArrayOf(5, 3, 8, 4, 2)
    val sortedArray = sortArray(array)
    println(sortedArray.joinToString(", "))
}
